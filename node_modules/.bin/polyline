#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zakiyys/Documents/App/zakiyys.github.io/node_modules/.pnpm/@mapbox+polyline@1.2.0/node_modules/@mapbox/polyline/bin/node_modules:/Users/zakiyys/Documents/App/zakiyys.github.io/node_modules/.pnpm/@mapbox+polyline@1.2.0/node_modules/@mapbox/polyline/node_modules:/Users/zakiyys/Documents/App/zakiyys.github.io/node_modules/.pnpm/@mapbox+polyline@1.2.0/node_modules/@mapbox/node_modules:/Users/zakiyys/Documents/App/zakiyys.github.io/node_modules/.pnpm/@mapbox+polyline@1.2.0/node_modules:/Users/zakiyys/Documents/App/zakiyys.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zakiyys/Documents/App/zakiyys.github.io/node_modules/.pnpm/@mapbox+polyline@1.2.0/node_modules/@mapbox/polyline/bin/node_modules:/Users/zakiyys/Documents/App/zakiyys.github.io/node_modules/.pnpm/@mapbox+polyline@1.2.0/node_modules/@mapbox/polyline/node_modules:/Users/zakiyys/Documents/App/zakiyys.github.io/node_modules/.pnpm/@mapbox+polyline@1.2.0/node_modules/@mapbox/node_modules:/Users/zakiyys/Documents/App/zakiyys.github.io/node_modules/.pnpm/@mapbox+polyline@1.2.0/node_modules:/Users/zakiyys/Documents/App/zakiyys.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@mapbox/polyline/bin/polyline.bin.js" "$@"
else
  exec node  "$basedir/../@mapbox/polyline/bin/polyline.bin.js" "$@"
fi
